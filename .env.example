# .env.example - Copy to .env and fill in your values

# Your Custom LLM Configuration
CUSTOM_API_KEY=your-api-key-here

# Model Endpoints (fill in your actual URLs)
BOTTLEROCKET_BASE_URL=https://your-bottlerocket-endpoint.com/v1
BOTTLEROCKET_MODEL=bottlerocket-model-name


GROK_BASE_URL=https://your-grok-endpoint.com/v1
GROK_MODEL=grok-model-name

# Default model to use (bottlerocket, tesla, or grok)
DEFAULT_MODEL=grok

# Optional: Tavily API Key for web search (research & react agents)
# Get from: https://tavily.com
TAVILY_API_KEY=your-tavily-api-key-here

# LangGraph CLI Configuration
PORT=2024
HOST=127.0.0.1
